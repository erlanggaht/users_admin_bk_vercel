{
    "version": 2,
    "headers": [

        {
            
          "source": "/api/(.*)"
    
        }
    
      ],
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
